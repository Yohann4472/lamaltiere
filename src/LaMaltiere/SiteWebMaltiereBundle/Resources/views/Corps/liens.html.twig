{% extends "LaMaltiereSiteWebMaltiereBundle::layout.html.twig" %}{% block section %}<section class="conteneur">	{% for lien in liens %}		<h2>		    {% if lien.id == 1 %}		        {% trans from "medias" %}liens.titre.siteclients{% endtrans %}		    {% elseif lien.id == 2 %}                {% trans from "medias" %}liens.titre.siteprofessionnels{% endtrans %}		    {% else %}                {% trans from "medias" %}liens.titre.referenceprofessionnelle{% endtrans %}		    {% endif %}		</h2>		<ul>			{% for site in lien.sites %}                <li>                    {% if site.adresse is empty %}                        {{ site.nom }}                    {% else %}                        <a href="{{ site.adresse }}" target=_blank">{{ site.nom }}</a>                    {% endif %}                </li>			{% endfor %}		</ul>	{% endfor %}</section>{% endblock %}