{% extends "LaMaltiereSiteWebMaltiereBundle::layout.html.twig" %}{% block section %}<section class="conteneur">	{% for lien in liens %}		<h2>{% if app.session.get('lang') == 'fr' %} {{ lien.nom_type_fr }} {% else %} {{ lien.nom_type_en }} {% endif %}</h2>		<ul>			{% for site in lien.sites %}                            <li>                                {% if site.adresse is empty %}                                    {{ site.nom }}                                {% else %}                                    <a href="{{ site.adresse }}" target=_blank">{{ site.nom }}</a>                                {% endif %}                            </li>			{% endfor %}		</ul>	{% endfor %}</section>{% endblock %}