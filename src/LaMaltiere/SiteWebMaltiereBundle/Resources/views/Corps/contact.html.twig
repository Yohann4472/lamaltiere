{% extends "LaMaltiereSiteWebMaltiereBundle::layout.html.twig" %}{% block section %}<section class="conteneur">	<h2>{% if app.session.get('lang') == 'fr' %}Nous contacter{% else %}Contact us{% endif %}</h2>	<div class="presentation">		<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2648.083793550389!2d-0.9133105000000183!3d48.416535599999946!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x7d3fdfca388b58fd!2sSAS+La+Malti%C3%A8re%2FBarque+aluminium%2FAlu+Speed+Boat%2FAluminium+boat!5e0!3m2!1sfr!2sfr!4v1397942476278" frameborder="0" style="border:0"></iframe>		<p>{{ entreprise }}<br />		{{ adresse }}<br />		{{ ville }}<br />		{{ telephone }}<br />		<a href="#">@ : {{ mail }}</a><br />		{{ skype }}<br /><br />		{{ siret }}<br />		{{ ape }}</p>	</div>	<h2>{% if app.session.get('lang') == 'fr' %}Télécharger notre catalogue et bon de commande{% else %}Download our catalogue and order form{% endif %}</h2>	<center>			<figure>				<a href="{{ asset('bundles/lamaltieresitewebmaltiere/pdf/CATALOGUE.pdf') }}" target="_blank">					<img src="{{ asset('bundles/lamaltieresitewebmaltiere/images/icones/catalogue.png') }}" width=80 height=105 border=0 alt="{% if app.session.get('lang') == 'fr' %}Catalogue{% else %}Catalogue{% endif %}" title="{% if app.session.get('lang') == 'fr' %}Catalogue{% else %}Catalogue{% endif %}">				</a>			  <figcaption>{% if app.session.get('lang') == 'fr' %}Catalogue{% else %}Catalogue{% endif %}</figcaption>			</figure>			<figure>				<a href="{{ asset('bundles/lamaltieresitewebmaltiere/pdf/bondecommande.pdf') }}" target="_blank">					<img src="{{ asset('bundles/lamaltieresitewebmaltiere/images/icones/commande.png') }}" width=80 height=105 border=0 alt="{% if app.session.get('lang') == 'fr' %}Bon de commande{% else %}Order form{% endif %}" title="{% if app.session.get('lang') == 'fr' %}Bon de commande{% else %}Order form{% endif %}">				</a>			  <figcaption>{% if app.session.get('lang') == 'fr' %}Bon de commande{% else %}Order form{% endif %}</figcaption>			</figure>	</center>	<div class="presentation">		{% if app.session.get('lang') == 'fr' %}			<p>Remplir le bon de commande et renvoyez le à SAS LA MALTIERE.<br />			Pour toute information tarifaire et/ou complémentaire sur nos produits, n'hésitez pas à nous contacter.</p>		{% else %}			<p>Fill the order form and send it back to SAS LA MALTIERE.<br />			For any prices or additional information about our products please contact us.</p>		{% endif %}	</div></section>{% endblock %}